{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "HostedServicesSelector": {
    "IntegrationEventsPublisher": false,
    "PublishIntegrationEventsBackgroundService": false,
    "ConsumeIntegrationEventsBackgroundService": false
  },
  "JwtSettings": {
    "IdentityUrl": "http://localhost:5208",
    "Secret": "secret-not-so-super-secret-key-that-is-long-enough",
    "TokenExpirationInMinutes": 60,
    "Issuer": "UserManagement",
    "Audience": "NexusManagement",
    "RequireHttps": false
  },
  "MessageBrokerPublisher": {
    "Publisher": "clean-template",
    "ExchangeName": "Clean-Template-Events"
  },
  "MessageBrokerConsumer": {
    "Queues": [],
    "ServiceOptions": {
      "InitialDelayInSeconds": 30,
      "WaitForNextTickInSeconds": 20
    }
  },
  "PublishIntegrationEvents": {
    "KeepOutboxRecordsEnabled": true,
    "KeepOutboxRecordsInDays": 7,
    "ServiceOptions": {
      "InitialDelayInSeconds": 10,
      "WaitForNextTickInSeconds": 20
    }
  },
  "EventualConsistency": {
    "KeepHistoryEnabled": true,
    "KeepHistoryInDays": 7
  }
}
